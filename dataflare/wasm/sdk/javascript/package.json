{
  "name": "@dataflare/plugin-sdk",
  "version": "1.0.0",
  "description": "DataFlare WASM Plugin SDK for JavaScript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dataflare",
    "plugin",
    "wasm",
    "webassembly",
    "data-processing",
    "etl"
  ],
  "author": "DataFlare Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dataflare/dataflare.git",
    "directory": "wasm/sdk/javascript"
  },
  "bugs": {
    "url": "https://github.com/dataflare/dataflare/issues"
  },
  "homepage": "https://dataflare.io",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rollup": "^4.0.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@wasmer/wasi": "^1.2.0",
    "@wasmer/wasmfs": "^0.12.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
